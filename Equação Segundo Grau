#include <stdio.h>
#include <math.h>
int main () {
	int a, b, c, delta, x1, x2;
	printf ("insira a variavel a: \n");
	scanf ("%d", &a);
	printf ("insira a variavel b: \n");
	scanf ("%d", &b);
	printf ("insira a variavel c: \n");
	scanf ("%d", &c);
	delta = (b*b) - (4*a*c);
	x1 = (-b + (sqrt(delta)))/2*a;
	x2 = (-b - (sqrt(delta)))/2*a;
	printf ("Os valores de X sao %d e %d \n", x1, x2);
	return 0;
}
